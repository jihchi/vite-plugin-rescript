{
  "name": "@jihchi/vite-plugin-rescript",
  "version": "5.3.1",
  "keywords": [
    "rollup-plugin",
    "vite-plugin",
    "ReScript",
    "ReasonML",
    "BuckleScript"
  ],
  "homepage": "https://github.com/jihchi/vite-plugin-rescript",
  "repository": {
    "type": "git",
    "url": "https://github.com/jihchi/vite-plugin-rescript.git"
  },
  "license": "MIT",
  "author": "jihchi",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "pnpm clean",
    "build": "tsup",
    "clean": "rimraf dist",
    "format": "prettier --write src test && pnpm dlx sort-package-json",
    "start": "pnpm build -- --watch",
    "test": "vitest run"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "execa": "^5.1.1",
    "npm-run-path": "^4.0.1"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "^5.0.0",
    "@vitest/coverage-v8": "^0.34.5",
    "prettier": "3.0.3",
    "rimraf": "^3.0.2",
    "tsup": "^6.7.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.5"
  },
  "peerDependencies": {
    "rescript": "^9 || ^10 || >=11.0.0-alpha",
    "vite": "^3 || ^4"
  },
  "packageManager": "pnpm@8.7.6",
  "engines": {
    "node": "^16 || ^18 || ^20"
  }
}
